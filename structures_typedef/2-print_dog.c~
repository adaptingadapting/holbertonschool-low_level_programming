#include "dog.h"
#include <stdio.h>

/**
 *print_dog - prints dog
 *@d: dog
 */
void print_dog(struct dog *d)
{
if (!d)
return;
if (!(d->name))
printf("Name: (nil)\n");
printf("Name: %s\n", d->name);
printf("Age: %f.1\n", d->age);
if (!(d->owner))
printf("Owner: (nil)\n");
printf("Owner: %s\n", d->owner);
}
